<!DOCTYPE HTML>
<html lang="en-us">
	<head>
		<title>DenúnciasJP</title>
		<meta charset="UTF-8">
		<link rel="stylesheet" type="text/css" href="css/style.css">
		<link rel="stylesheet" type="text/css" href="css/estilo.css">
		<link href='http://fonts.googleapis.com/css?family=Open+Sans:300' rel='stylesheet' type='text/css'>
	</head>
	<body>

		<!-- TOPO DA PÁGINA -->
		<section id="home">
			<div class="topo">
				<img src="../img/denunciasjp.png" />
				<form action="#registrar-denuncia">
					<input type="text" placeholder="Pesquisar denúncia..">
					<button>buscar</button>
				</form>
				<ul>
					<li class="home"><a href="http://localhost:4567">Página Inicial</a></li>
					<li class="denuncia"><a href="#registrar-denuncia">Registrar Denúncia</a></li>
					<li class="cadastre-se">Cadastre-se</li>
					<li class="como-funciona">Como Funciona</li>
				</ul>
				<img src="../img/barra-cima.jpg" / class="barra">
			</div>
		</section>
		<!-- FIM DO TOPO DA PÁGINA -->

		<!-- CONTEÚDO -->
		<section id="content">
			<div class="titulo">Denúncias Recentes</div>
			<div class="denuncias-recentes">

				<script type="text/x-handlebars-template" id="denuncias-list">
				
					{{#each denuncias}}
					{{resumo}}
					{{denuncia}}
					{{/each}}
				</script>
			</div>
		</section>
		<!-- FIM DO CONTEÚDO -->

		<!-- MENU -->
		<section id="menu">
			<div class="menu">
				haha
			</div>
		</section>
		<!-- FIM DO MENU -->

		<!-- FOOTER -->
		</section>
		<section id="footer">
			<img src="../img/barra-baixo.jpg" />
		</section>

		<!-- FIM DO FOOTER -->

	

		<script type="text/javascript" src="js/libs/jquery.js"></script>
		<script type="text/javascript" src="js/libs/json2.js"></script>
		<script type="text/javascript" src="js/libs/underscore.js"></script>
		<script type="text/javascript" src="js/libs/backbone.js"></script>
		<script type="text/javascript" src="js/libs/handlebars.js"></script>
		<script type="text/javascript" src="js/app.js"></script>
	</body>
</html>